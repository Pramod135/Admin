
@model Admin_Management.Models.TblPage


@*html code convert to web*@
@if (Model!=null && Model.Name != null && Model.IsActive==true)
{   
    @Html.Raw(System.Web.HttpUtility.HtmlDecode(Model.PageContent))
    @* @Model.PageContent*@
}

else{

    <figure class="wp-block-image">
        <picture decoding="async">
            <source type="image/webp" srcset="https://colorlib.com/wp/wp-content/uploads/sites/2/404-error-template-11.png.webp">
            <img decoding="async" src="https://colorlib.com/wp/wp-content/uploads/sites/2/404-error-template-11.png" alt="" width="1920" height="933" loading="lazy" fetchpriority="low">
        </picture>
    </figure>

